{
  "name": "faqmaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:railway": "railway run pnpm dev",
    "build": "pnpm migrate:deploy && next build",
    "start": "next start --port ${PORT-3000}",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "generate": "prisma generate",
    "push": "prisma db push",
    "format": "prisma format",
    "type": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "prettier:check": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --check",
    "prettier:write": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "5.2.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@stripe/stripe-js": "^2.1.6",
    "@tanstack/react-query": "^4.33.0",
    "@uiw/react-color-sketch": "^1.3.3",
    "@uiw/react-markdown-preview": "^4.1.15",
    "@uiw/react-md-editor": "^3.23.5",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "jotai": "^2.4.3",
    "lucide-react": "^0.269.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-connect": "^0.13.0",
    "next-remove-imports": "^1.0.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "rehype-sanitize": "^6.0.0",
    "slugify": "^1.6.6",
    "stripe": "^13.6.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@swc-jotai/react-refresh": "^0.1.0",
    "@types/node": "^20.5.6",
    "@types/react": "^18.0.27",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "5.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "pnpm lint:fix",
      "pnpm prettier:write"
    ]
  }
}
