{
  "name": "faqmaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:railway": "railway run pnpm dev",
    "compose": "docker compose up -d",
    "build": "pnpm migrate:deploy && next build",
    "start": "next start --port ${PORT-3000}",
    "test": "pnpm dlx playwright test",
    "test:headed": "pnpm exec playwright test --headed",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "push": "prisma db push",
    "format": "prisma format",
    "type": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "prettier:check": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --check",
    "prettier:write": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "prisma generate && husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.5.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@slack/webhook": "^7.0.1",
    "@stripe/stripe-js": "^2.1.6",
    "@tanstack/react-query": "^5.14.1",
    "@uiw/react-color-sketch": "^2.0.5",
    "@uiw/react-markdown-preview": "^5.0.6",
    "@uiw/react-md-editor": "^4.0.2",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "jotai": "^2.4.3",
    "lucide-react": "^0.303.0",
    "micro": "^10.0.1",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "next-connect": "^0.13.0",
    "next-remove-imports": "^1.0.12",
    "papaparse": "^5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.45.4",
    "react-paginate": "^8.2.0",
    "rehype-sanitize": "^6.0.0",
    "slugify": "^1.6.6",
    "sonner": "^1.0.3",
    "stripe": "^14.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.6",
    "vaul": "^0.8.0",
    "wcag-contrast": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@playwright/test": "^1.39.0",
    "@swc-jotai/react-refresh": "^0.1.0",
    "@types/node": "^20.5.6",
    "@types/react": "^18.0.27",
    "autoprefixer": "^10.4.16",
    "eslint": "8.48.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "5.5.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "pnpm lint:fix",
      "pnpm prettier:write"
    ]
  }
}
