{
  "name": "faqmaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:railway": "railway run pnpm dev",
    "build": "pnpm migrate:deploy && next build",
    "start": "next start --port ${PORT-3000}",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "generate": "prisma generate",
    "push": "prisma db push",
    "type": "tsc --noEmit",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --ignore-path .gitignore --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format:fix": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.1",
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "5.2.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tanstack/react-query": "^4.33.0",
    "@tremor/react": "^3.6.6",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.269.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@types/react": "^18.0.27",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "prisma": "5.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
